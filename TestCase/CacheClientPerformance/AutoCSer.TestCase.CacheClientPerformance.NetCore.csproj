  </ItemGroup>

  <ItemGroup>
    <Compile Include="Array.cs" />
    <Compile Include="Binary.cs" />
    <Compile Include="Dictionary.cs" />
    <Compile Include="Json.cs" />
    <Compile Include="Message.cs" />
    <Compile Include="MesssageDistributor.cs" />
    <Compile Include="Program.cs" />
    <Compile Include="Properties\AssemblyInfo.cs" />
    <Compile Include="MessageQueue.cs" />
    <Compile Include="Test.cs" />
    <Compile Include="CallbackType.cs" />
    <Compile Include="TestType.cs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\AutoCSer\CacheServer\AutoCSer.CacheServer.NetStandard.csproj" />
  </ItemGroup>

</Project>
